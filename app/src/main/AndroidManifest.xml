<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schou.push">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GroupActivity" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".NewMemberActivity" />
        <activity android:name=".NewGroupActivity" />
        <activity android:name=".MembersInGroupActivity" />
        <activity android:name=".NewMessageActivity"></activity>
    </application>

</manifest>